/* pages/study/practice/practice.wxss */
.module {
  width: 90%;
  background-color: white;
  height: 200rpx;
  margin: 0 auto;
  border-radius: 30rpx;
  margin-top: 40rpx;
  display: flex;
  align-items: center;
  padding: 5rpx 10rpx 5rpx 10rpx;
}

image {
  width: 30%;
  height: 70%;
  object-fit: contain;
  margin: 0 40rpx 0 20rpx;
}

.title_progress {
  width: 70%;
  height: 80%;
  position: relative;
}
text{
  width: 80%;
  height: 20px;
  text-align: center;
  position: absolute;
  top: 20%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
progress {
  width: 100%;
  position: absolute;
  bottom: 0;
  font-weight: bold;
}