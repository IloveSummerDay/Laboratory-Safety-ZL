/* pages/exam-modules/initial-entry/index.wxss */
.exam_module_box {
  background-color: white;
  padding: 20rpx;
  border-bottom: 1px solid #ccc;
  position: relative;
}

.title {
  font-size: 40rpx;
  color: #105a9d;
  font-weight: bold;
  margin-bottom: 20rpx;
}
.start_time,
.end_time {
  color: rgb(196, 196, 196);
}
.complete_title {
  color: rgb(52, 182, 84);
}
.complete_icon {
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  right: 20rpx;
  bottom: 20rpx;
  transform: rotateZ(10deg);
}
