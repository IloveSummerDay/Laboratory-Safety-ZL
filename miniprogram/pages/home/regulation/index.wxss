/* pages/home/regulation/index.wxss */
.item {
  width: 85%;
  padding: 0 10rpx 0 30rpx;
  min-height: 80rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  border-radius: 20rpx;
  display: flex;
  align-items: center;
  background-color: white;
}
.item text {
  width: 80%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.shadow_inside {
  box-shadow: inset 0 0 10px rgba(95, 95, 95, 0.2);
}
